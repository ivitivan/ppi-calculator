    "prepublish": "npm run build",
    "test": "ava"
  },
  "keywords": [
    "ppi",
    "calculator"
  ],
  "author": "Vitaly Ivanov <ivitivan@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "minimist": "1.2.0"
  },
  "devDependencies": {
    "ava": "0.16.0",
    "babel": "6.5.2",
    "babel-cli": "6.11.4",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-es2015-rollup": "1.2.0",
    "babel-register": "6.11.6",
    "rollup": "0.34.10",
    "rollup-plugin-babel": "2.6.1",
    "sinon": "1.17.5"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": {
      "presets": [
        "es2015"
      ],
      "babelrc": false
    }
  }
