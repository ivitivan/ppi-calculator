{
  "name": "ppi-calculator",
  "version": "1.0.0",
  "description": "PPI Calculator",
  "files": [
    "./dist/bin.js"
  ],
  "main": "index.js",
  "bin": {
    "ppi-calculator": "./dist/bin.js"
  },
  "scripts": {
